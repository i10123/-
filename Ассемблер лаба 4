masm
model small
.386
.stack 400h

.data
    sign db ?
    X dw ?
    Y dw ?
    F dw ?
    msgX db 'Input X: $'
    msgY db 'Input Y: $'
    msgExample db 'Primer: F = (4596 - 589) / X + (Y * 2385)',10, 13,'$'
.code

InputNumber proc
    mov sign, 0
    
    mov ah, 01h
    int 21h
    cmp al, '-'
    jne coninput
    mov sign, 1
    
    mov ah, 01h
    int 21h
coninput:
    sub al, 30h
    mov ah, 0
    mov bx, 10
    mov cx, ax
Input:
    mov ah, 01h
    int 21h
    cmp al, 0Dh
    je EndPrint
    sub al, 30h
    cbw
    xchg ax, cx
    mul bx
    add cx, ax
    jmp Input
EndPrint:
    cmp sign, 1
    jne final
    neg cx
final:
    ret
InputNumber endp

start:
    mov ax, @data
    mov ds, ax
    
    mov dx, offset msgExample
    mov ah, 09h
    int 21h
    
    mov dx, offset msgX
    mov ah, 09h
    int 21h
    call InputNumber
    mov X, cx
    
    mov dx, offset msgY
    mov ah, 09h
    int 21h
    call InputNumber
    mov Y, cx

    mov ax, 4596
    sub ax, 589
    cwd
    idiv X
    mov bx, ax
    mov ax, Y
    imul ax, 2385
    add bx, ax
    mov F, bx

    mov ax, F
    cmp ax, 0
    jnl Positive
    push ax
    mov dl, '-'
    mov ah, 02h
    int 21h
    pop ax
    neg ax
Positive:
    mov bx, 10
    push -1
ConvertLoop:
    xor dx, dx
    div bx
    push dx
    test ax, ax
    jnz ConvertLoop
PrintLoop:
    pop dx
    cmp dx, -1
    je Exit
    add dl, '0'
    mov ah, 02h
    int 21h
    jmp PrintLoop
Exit:
    mov ax, 4C00h
    int 21h
end start
